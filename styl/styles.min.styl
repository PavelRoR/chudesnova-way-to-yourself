*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: Di
	color #3E2E14
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.general-title
	text-align: center
	font-size: 28px
	font-family: DiB
	// margin-bottom: 35px
	&:after
		content: ''
		display block
		width 100%
		max-width 500px
		height 5px
		background-color: #3E2E14
		margin 10px auto 55px
		transform: skewX(-45deg);
.button
	display block
	margin: 0 auto
	position relative
	text-align: center
	color myG - 50%
	text-transform: uppercase
	height 50px
	line-height 50px
	width 100%
	max-width 250px
	// text-shadow: 1px 1px 0 #3E2E14
	transition: all .3s ease
	&:before
		content: ''
		position: absolute
		top: 0
		left 0
		bottom 0
		right 0
		width 100%
		height 100%
		// background-color: myG
		background-image: linear-gradient(to top, myG 0%, rgba(myG,.6) 100%)
		z-index -1
		transform: skewX(-25deg);
		border: 1px solid myG - 50%
	&:focus
		text-decoration: none
		color myG - 50%
	&:hover
		text-decoration: none
		color myG - 50%
		transform: translateY(-3px)
.general-list
	li
		padding-left: 24px
		margin-bottom: 20px
		background-image url(../img/arrow.png)
		background-repeat no-repeat
		background-position: left top 5px
		list-style: none
		&:last-child
			margin-bottom: 0

